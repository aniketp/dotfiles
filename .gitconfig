[user]
    name = Aniket Pandey
    email = aniketp@freebsd.org

[url "https://github.com/"]
    insteadOf = gh:

[color]
    ui = true

[color "branch"]
    current = yellow reverse
    local = yellow
    remote = green

[color "diff"]
    meta = yellow bold
    frag = magenta bold
    old = red bold
    new = green bold

[color "status"]
    added = yellow
    changed = green
    untracked = red

[alias]
    url = remote get-url
    back = checkout master
    co = commit

[push]
    default = simple
